public class Calculadora {
  public Calculadora() {}
  public static void main(String[] args) {
	System.out.println("Resultado de la primera suma: " + sumar(1,3));
	System.out.println("Resultado de la primera suma: " + sumar(1,3));
  }
  public static int sumar(int a, int b) {
    return a + b;
  }
}
